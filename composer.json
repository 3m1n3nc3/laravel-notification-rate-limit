{
    "name": "jamesmills/laravel-notification-throttle",
    "description": "Rate limiter to avoid flooding users with duplicate notifications.",
    "keywords": [
        "jamesmills",
        "laravel-notification-throttle"
    ],
    "homepage": "https://github.com/jamesmills/laravel-notification-throttle",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "James Mills",
            "email": "james@jamesmills.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Jamesmills\\LaravelNotificationThrottle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jamesmills\\LaravelNotificationThrottle\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jamesmills\\LaravelNotificationThrottle\\LaravelNotificationThrottleServiceProvider"
            ],
            "aliases": {
                "LaravelNotificationThrottle": "Jamesmills\\LaravelNotificationThrottle\\LaravelNotificationThrottleFacade"
            }
        }
    }
}
